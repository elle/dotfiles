# contains values used by other scripts and applications
export PATH=/usr/local/bin:/usr/local/sbin:$PATH
# export PATH=/usr/local/bin:/usr/local/sbin:/usr/local/mysql/bin:/opt/local/bin:$PATH

export EDITOR='mate -w'
export CLICOLOR=1;

# When installing node -v0.4.9, go this:
# Please add /usr/local/lib/node to your NODE_PATH environment variable to have node libraries picked up.
export NODE_PATH=/usr/local/lib/node$NODE_PATH
